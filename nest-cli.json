{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/ethos-codespace/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/ethos-codespace/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/ethos-codespace",
  "projects": {
    "app-ethos": {
      "type": "application",
      "root": "apps/app-ethos",
      "entryFile": "main",
      "sourceRoot": "apps/app-ethos/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app-ethos/tsconfig.app.json"
      }
    },
    "buffer-api": {
      "type": "application",
      "root": "apps/buffer-api",
      "entryFile": "main",
      "sourceRoot": "apps/buffer-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/buffer-api/tsconfig.app.json"
      }
    },
    "certificados-cat": {
      "type": "application",
      "root": "apps/certificados-cat",
      "entryFile": "main",
      "sourceRoot": "apps/certificados-cat/src",
      "compilerOptions": {
        "tsConfigPath": "apps/certificados-cat/tsconfig.app.json"
      }
    },
    "estrutura-api": {
      "type": "application",
      "root": "apps/estrutura-api",
      "entryFile": "main",
      "sourceRoot": "apps/estrutura-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/estrutura-api/tsconfig.app.json"
      }
    },
    "ethos-codespace": {
      "type": "application",
      "root": "apps/ethos-codespace",
      "entryFile": "main",
      "sourceRoot": "apps/ethos-codespace/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ethos-codespace/tsconfig.app.json"
      }
    },
    "ethos-mobile": {
      "type": "application",
      "root": "apps/ethos-mobile",
      "entryFile": "main",
      "sourceRoot": "apps/ethos-mobile/src",
      "compilerOptions": {
        "assets": [
          "templates/*"
        ],
        "tsConfigPath": "apps/ethos-mobile/tsconfig.app.json"
      }
    },
    "ethos-producao": {
      "type": "application",
      "root": "apps/ethos-producao",
      "entryFile": "main",
      "sourceRoot": "apps/ethos-producao/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ethos-producao/tsconfig.app.json"
      }
    },
    "ethos-relogio-de-ponto-api": {
      "type": "application",
      "root": "apps/ethos-relogio-de-ponto-api",
