      "compilerOptions": {
        "tsConfigPath": "apps/ethos-relogio-de-ponto/tsconfig.app.json"
      }
    },
    "ethos-relogio-de-ponto-master": {
      "type": "application",
      "root": "apps/ethos-relogio-de-ponto-master",
      "entryFile": "main.master",
      "sourceRoot": "apps/ethos-relogio-de-ponto/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ethos-relogio-de-ponto/tsconfig.master.json"
      }
    },
    "ethos-relogio-de-ponto-worker": {
      "type": "application",
      "root": "apps/ethos-relogio-de-ponto-worker",
      "entryFile": "main.worker",
      "sourceRoot": "apps/ethos-relogio-de-ponto/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ethos-relogio-de-ponto/tsconfig.worker.json"
      }
    },
    "modules": {
      "type": "library",
      "root": "libs/modules",
      "entryFile": "index",
      "sourceRoot": "libs/modules/src",
      "compilerOptions": {
        "tsConfigPath": "libs/modules/tsconfig.lib.json"
      }
    },
    "notas-maker": {
      "type": "application",
      "root": "apps/notas-maker",
      "entryFile": "main",
      "sourceRoot": "apps/notas-maker/src",
      "compilerOptions": {
        "assets": [
          "templates/*"
        ],
        "tsConfigPath": "apps/notas-maker/tsconfig.app.json"
      }
    },
    "notas-maker-worker": {
      "type": "application",
      "root": "apps/notas-maker-worker",
      "entryFile": "main.worker",
      "sourceRoot": "apps/notas-maker/src",
      "compilerOptions": {
        "assets": [
          "templates/*"
        ],
        "tsConfigPath": "apps/notas-maker/tsconfig.worker.json"
      }
    },
    "notificacao-api": {
      "type": "application",
      "root": "apps/notificacao-api",
