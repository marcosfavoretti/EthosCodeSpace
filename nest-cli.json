      }
    },
    "planejador-importador": {
      "type": "application",
      "root": "apps/planejador-importador",
      "entryFile": "main.importador",
      "sourceRoot": "apps/planejador/src",
      "compilerOptions": {
        "tsConfigPath": "apps/planejador/tsconfig.importador.json"
      }
    },
    "planejador-worker": {
      "type": "application",
      "root": "apps/planejador-worker",
      "entryFile": "main.worker",
      "sourceRoot": "apps/planejador/src",
      "compilerOptions": {
        "tsConfigPath": "apps/planejador/tsconfig.worker.json"
      }
    },
    "user-authenticator": {
      "type": "application",
      "root": "apps/user-authenticator",
      "entryFile": "main",
      "sourceRoot": "apps/user-authenticator/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-authenticator/tsconfig.app.json",
        "assets": [
          "views/*.hbs"
        ]
      }
    },
    "wifi-ethos": {
      "type": "application",
      "root": "apps/wifi-ethos",
      "entryFile": "main",
      "sourceRoot": "apps/wifi-ethos/src",
      "compilerOptions": {
        "tsConfigPath": "apps/wifi-ethos/tsconfig.app.json"
      }
    }
  }
}